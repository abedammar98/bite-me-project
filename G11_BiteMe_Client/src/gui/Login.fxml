<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" stylesheets="@Login.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LoginController">
   <children>
      <Button id="btnExit" fx:id="btnExit" layoutX="335.0" layoutY="471.0" mnemonicParsing="false" onAction="#getBackBtn" text="Exit" />
      <Button fx:id="btnLogin" layoutX="317.0" layoutY="408.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <HBox layoutX="134.0" layoutY="227.0" prefHeight="136.0" prefWidth="442.0" spacing="37.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="30.0">
               <children>
                  <Label text="Username:" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="19.0" />
                     </font>
                  </Label>
                  <Label text="Password:" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="19.0" />
                     </font>
                  </Label>
                  <Label text="Type:" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="19.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox prefHeight="136.0" prefWidth="189.0" spacing="20.0">
               <children>
                  <TextField fx:id="Usernametxt" />
                  <PasswordField fx:id="Passwordtxt" />
                  <ComboBox fx:id="UserTypecb" prefWidth="150.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
