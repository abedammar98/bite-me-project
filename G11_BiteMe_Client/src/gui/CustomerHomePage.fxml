<BorderPane prefHeight="855.0" prefWidth="1305.0" stylesheets="@CustomerHomePage.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CustomerHomePageController">
   <left>
      <VBox prefHeight="855.0" prefWidth="250.0" BorderPane.alignment="CENTER">
